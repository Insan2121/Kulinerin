<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kulinerin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/upup">

        <activity android:name=".features.transactionlist.TransactionList">
        </activity>
        <activity android:name=".features.detailtransaction.DetailTransaction">

        </activity>
        <activity android:name=".features.mainscreen.payment.PaymentActivity"></activity>
        <activity
            android:name=".features.mainscreen.checkout.CheckoutActivity"
            android:theme="@style/upup"
            >

        </activity>
        <activity
            android:name=".features.detailproduct.DetailProductActivity"
            />
        <activity
            android:name=".features.mainscreen.MainScreenActivity"
            android:theme="@style/upup">
        </activity>
        <activity android:name=".features.authentication.Login" />
        <activity
            android:name=".features.authentication.Register"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".CustomerWelcome"
            android:theme="@style/upup" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/upup" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>